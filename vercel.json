{
  "public": true,
  "cleanUrls": true,
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "www.muffinpanrecipes.com"
        }
      ],
      "destination": "https://muffinpanrecipes.com/$1",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/src/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/src/$1"
    }
  ]
}

